        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Emp_Main_Activity" />
        <activity android:name=".Emp_View_Activity" />
        <activity android:name=".Emp_Add_Activity" />
        <activity android:name=".View_R_Activity" />
        <activity android:name=".Add_R_Activity" />
        <activity android:name=".R_Handle_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>